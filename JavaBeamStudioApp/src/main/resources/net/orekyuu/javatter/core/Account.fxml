<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="net.orekyuu.javatter.core.config.AccountController">
    <children>
    <Text layoutX="14.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title" text="認証済みアカウント一覧" />
        <ListView fx:id="accountList" prefWidth="380" prefHeight="238.0" maxWidth="Infinity" AnchorPane.bottomAnchor="140.0" AnchorPane.leftAnchor="10.0"
                  AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="30.0"/>
        <Button onAction="#addAccount" mnemonicParsing="false" styleClass="add" text="新規アカウントの認証"
                AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="10.0"/>
        <Button fx:id="deleteAccountButton" onAction="#deleteAccount" mnemonicParsing="false"
                styleClass="delete" text="アカウントの削除" AnchorPane.bottomAnchor="80.0" AnchorPane.rightAnchor="10.0" />
  </children>
</AnchorPane>
