<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="net.orekyuu.javatter.core.config.GeneralConfigPresenter">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="システム">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <CheckBox fx:id="checkTweet" mnemonicParsing="false" text="ツイート時に確認を行う">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </CheckBox>
        <CheckBox fx:id="checkReply" mnemonicParsing="false" text="リプライ時に確認を行う">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </CheckBox>
        <CheckBox fx:id="checkRT" mnemonicParsing="false" text="リツイート時に確認を行う">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </CheckBox>
        <CheckBox fx:id="checkFav" mnemonicParsing="false" text="お気に入り時に確認を行う">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </CheckBox>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="見た目">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <Label maxHeight="1.7976931348623157E308" text="名前の表示形式：">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </Label>
        <ChoiceBox fx:id="nameDisplayType" prefWidth="150.0">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </ChoiceBox>
        <CheckBox fx:id="isExpandURL" mnemonicParsing="false" text="URLの展開">
            <VBox.margin>
                <Insets left="20.0"/>
            </VBox.margin>
        </CheckBox>
        <HBox>
            <children>
                <Button onAction="#save" mnemonicParsing="false" text="保存"/>
                <Button onAction="#cancel" mnemonicParsing="false" text="キャンセル"/>
            </children>
            <VBox.margin>
                <Insets left="20.0" top="50.0"/>
            </VBox.margin>
        </HBox>
    </children>
</VBox>
